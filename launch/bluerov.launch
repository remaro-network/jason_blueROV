<launch>
  <arg name="fcu_url" default="udp://:14551@:14555"/>

  <include file="$(find mavros)/launch/apm.launch">
    <arg name="fcu_url" value="$(arg fcu_url)"/>
  </include>

  <node name="hw_bridge" pkg="jason_ros" type="hw_bridge.py"
    args="-a $(find jason_bluerov)/config/mission_actions_manifest
    -p $(find jason_bluerov)/config/mission_perceptions_manifest"/>

  <node name="jason_agent" pkg="jason_ros" type="gradle_starter.sh"
    args="$(find jason_bluerov)/src/bluerov/build.gradle"/>

</launch>
